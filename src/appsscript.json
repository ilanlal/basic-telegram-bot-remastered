{
  "timeZone": "America/New_York",
  "exceptionLogging": "STACKDRIVER",
  "runtimeVersion": "V8",
  "webapp": {
    "executeAs": "USER_DEPLOYING",
    "access": "ANYONE_ANONYMOUS"
  },
  "oauthScopes": [
    "https://www.googleapis.com/auth/script.locale",
    "https://www.googleapis.com/auth/spreadsheets.currentonly",
    "https://www.googleapis.com/auth/script.external_request"
  ],
  "addOns": {
    "common": {
      "name": "Basic Telegram Bot",
      "logoUrl": "https://raw.githubusercontent.com/ilanlal/basic-telegram-bot-remastered/main/assets/logo24.png",
      "layoutProperties": {
        "primaryColor": "#2772ed",
        "secondaryColor": "#1010ff"
      },
      "openLinkUrlPrefixes": [
        "https://raw.githubusercontent.com/ilanlal/",
        "https://www.github.com/ilanlal/",
        "https://github.com/ilanlal/",
        "https://www.easyadm.com",
        "https://easyadm.com"
      ],
      "homepageTrigger": {
        "runFunction": "AddonHandler.Events.onHomePageOpen"
      },
      "useLocaleFromApp": true,
      "universalActions": [
        {
          "label": "Account & Settings",
          "runFunction": "AddonHandler.Events.onAccountCardOpen"
        },
        {
          "label": "About",
          "runFunction": "AddonHandler.Events.onAboutCardOpen"
        },
        {
          "label": "Help & Support",
          "openLink": "https://github.com/ilanlal/basic-telegram-bot-remastered#readme"
        },
        {
          "label": "Report an issue",
          "openLink": "https://github.com/ilanlal/basic-telegram-bot-remastered/issues"
        }
      ]
    },
    "sheets": {}
  },
  "urlFetchWhitelist": [
    "https://api.telegram.org/",
    "https://raw.githubusercontent.com/ilanlal/",
    "https://www.github.com/ilanlal/",
    "https://github.com/ilanlal/",
    "https://www.easyadm.com/",
    "https://easyadm.com/"
  ]
}